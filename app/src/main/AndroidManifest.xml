<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.savita.login">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Login">
        <activity
            android:name="com.savita.navigate.UploadedDetails"
            android:exported="false" />
        <activity
            android:name="com.savita.navigate.StatusOfApplication"
            android:exported="false" />
        <activity
            android:name="com.savita.navigate.UserSelect"
            android:exported="false" />
        <activity
            android:name="com.savita.navigate.Navigationactivity"
            android:exported="false"
            android:label="@string/title_activity_navigationactivity"
            android:theme="@style/Theme.Login.NoActionBar" />
        <activity
            android:name=".Payment"
            android:exported="false" />
        <activity
            android:name="com.savita.navigate.CanUploadDocument"
            android:exported="false" />
        <activity
            android:name="com.savita.navigate.UploadedDocument"
            android:exported="false" />
        <activity
            android:name=".RequireDetail"
            android:exported="false" />
        <activity
            android:name=".RequireDocument"
            android:exported="false" />
        <activity
            android:name=".ExamList"
            android:exported="false" />
        <activity
            android:name="com.savita.signin.ResetPassword"
            android:exported="false" />
        <activity
            android:name="com.savita.signin.MainActivity"
            android:exported="false" />
        <activity
            android:name="com.savita.signin.Register"
            android:exported="false" />
        <activity
            android:name=".Home"
            android:exported="true"
            android:theme="@style/Theme.Login.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>